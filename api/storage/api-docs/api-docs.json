{
    "openapi": "3.0.0",
    "info": {
        "title": "API Laravel Wallet",
        "description": "API documentation for user management and financial transactions.",
        "version": "1.0.0"
    },
    "paths": {
        "/api/accounts": {
            "get": {
                "tags": [
                    "Account"
                ],
                "summary": "Get all accounts of the authenticated user",
                "operationId": "13efa5706d1fae14fc1cf87c9f6b1427",
                "responses": {
                    "200": {
                        "description": "List of user accounts"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Account"
                ],
                "summary": "Create a new account",
                "operationId": "e974666bc6c050296a8ee09a3ab6533f",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Savings Account"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Account created successfully"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/accounts/{id}": {
            "get": {
                "tags": [
                    "Account"
                ],
                "summary": "Get a specific account of the authenticated user",
                "operationId": "552b29d7944dfff7a0c49abc83f44ea5",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Account details"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Account"
                ],
                "summary": "Delete an account if balance is zero",
                "operationId": "102c60c5fd1e94be8c71c4efbeb51bf8",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Account deleted successfully"
                    },
                    "400": {
                        "description": "Account must have zero balance to be deleted"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Register a new user",
                "operationId": "8a56853624e025573120a09a4c75d468",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "johndoe@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "123456"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User registered successfully"
                    },
                    "400": {
                        "description": "Error registering user"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Authenticate user and get JWT token",
                "operationId": "a3b306d14572d1f4bd6c064b3233e7b8",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "johndoe@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "123456"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "JWT token generated successfully"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/reversals/transaction": {
            "post": {
                "tags": [
                    "Reversal"
                ],
                "summary": "Reverse a transaction",
                "operationId": "22326ae9954335ccfde7ebcce89ba587",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "transaction_id",
                                    "reason"
                                ],
                                "properties": {
                                    "transaction_id": {
                                        "description": "ID of the transaction to be reversed",
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "reason": {
                                        "description": "Reason for the reversal",
                                        "type": "string",
                                        "example": "Sent to the wrong account"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Transaction reversed successfully"
                    },
                    "400": {
                        "description": "Invalid transaction or insufficient balance"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/reversals/deposit": {
            "post": {
                "tags": [
                    "Reversal"
                ],
                "summary": "Reverse a deposit",
                "operationId": "9ca4f1b1b57c4e65428d6d3574b2f898",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "deposit_id",
                                    "reason"
                                ],
                                "properties": {
                                    "deposit_id": {
                                        "description": "ID of the deposit to be reversed",
                                        "type": "integer",
                                        "example": 5
                                    },
                                    "reason": {
                                        "description": "Reason for the reversal",
                                        "type": "string",
                                        "example": "Deposit sent to the wrong account"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Deposit reversed successfully"
                    },
                    "400": {
                        "description": "Invalid deposit or insufficient balance"
                    }
                }
            }
        },
        "/api/transactions/transfer": {
            "post": {
                "tags": [
                    "Transaction"
                ],
                "summary": "Transfer money between accounts",
                "operationId": "c52a2fa87fe3a4d506bb209f47d65b2e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "sender_id",
                                    "recipient_id",
                                    "amount",
                                    "type"
                                ],
                                "properties": {
                                    "sender_id": {
                                        "description": "ID of the sender's account",
                                        "type": "integer",
                                        "example": 72
                                    },
                                    "recipient_id": {
                                        "description": "ID of the recipient's account",
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "amount": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 100
                                    },
                                    "type": {
                                        "type": "string",
                                        "example": "transfer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Transfer successful"
                    },
                    "400": {
                        "description": "Insufficient balance or invalid accounts"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/transactions/deposit": {
            "post": {
                "tags": [
                    "Transaction"
                ],
                "summary": "Deposit money into an account",
                "operationId": "b8dfc48af354a927aa7f0dcc4572d402",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "recipient_id",
                                    "amount",
                                    "type"
                                ],
                                "properties": {
                                    "recipient_id": {
                                        "description": "ID of the account receiving the deposit",
                                        "type": "integer",
                                        "example": 72
                                    },
                                    "amount": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 200
                                    },
                                    "type": {
                                        "type": "string",
                                        "example": "deposit"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Deposit successful"
                    },
                    "400": {
                        "description": "Invalid account or unauthorized deposit"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/profile": {
            "get": {
                "tags": [
                    "User"
                ],
                "summary": "Get authenticated user profile",
                "operationId": "4373251f8140344279ac32a1ac817cef",
                "responses": {
                    "200": {
                        "description": "User profile data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "John Doe"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "johndoe@example.com"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "User"
                ],
                "summary": "Update authenticated user profile",
                "operationId": "116515f53f0ecf2584e4a57a7e7db4ba",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "johndoe@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "newpassword"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Profile updated successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Profile updated successfully"
                                        },
                                        "user": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Error updating profile"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "User"
                ],
                "summary": "Delete authenticated user profile",
                "operationId": "988196d9c1409e9801268baec3747863",
                "responses": {
                    "200": {
                        "description": "Profile deleted successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Profile deleted successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Cannot delete profile with account balance"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "tags": [
        {
            "name": "Account",
            "description": "Account"
        },
        {
            "name": "Authentication",
            "description": "Authentication"
        },
        {
            "name": "Reversal",
            "description": "Reversal"
        },
        {
            "name": "Transaction",
            "description": "Transaction"
        },
        {
            "name": "User",
            "description": "User"
        }
    ],
    "security": [
        {
            "bearerAuth": []
        }
    ]
}